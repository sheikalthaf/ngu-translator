{
  "name": "ngu-translator",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "cp-file": "cp -fr projects/ngu-translator/src/assets/xml-serializer.ts node_modules/ngx-i18nsupport-lib/src/impl",
    "precommit": "run-s format:fix",
    "format:fix": "pretty-quick --staged",
    "format:all": "prettier --config ./.prettierrc --write \"projects/ngu-translator/src/{app,environments}/**/*{.ts,.js,.json,.css,.scss}\"",
    "build-site": "yarn run cp-file && ng build --project ngu-translator --prod --base-href https://sheikalthaf.github.io/ngu-translator"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.1.4",
    "@angular/cdk": "~7.2.0",
    "@angular/common": "~7.1.4",
    "@angular/compiler": "~7.1.4",
    "@angular/core": "~7.1.4",
    "@angular/forms": "~7.1.4",
    "@angular/http": "~7.1.4",
    "@angular/material": "^7.2.0",
    "@angular/platform-browser": "~7.1.4",
    "@angular/platform-browser-dynamic": "~7.1.4",
    "@angular/pwa": "^0.11.4",
    "@angular/router": "~7.1.4",
    "@angular/service-worker": "~7.1.4",
    "@ngrx/effects": "^7.0.0",
    "@ngrx/store": "^7.0.0",
    "core-js": "^2.5.4",
    "file-saver": "^2.0.0",
    "ngx-i18nsupport-lib": "^1.10.2",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.10.0",
    "@angular/cli": "~7.1.4",
    "@angular/compiler-cli": "~7.1.4",
    "@angular/language-service": "~7.1.4",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "husky": "^1.3.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.15.3",
    "pretty-quick": "^1.8.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.1"
  }
}
